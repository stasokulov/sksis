{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "no-cond-assign": 2,
        "no-console": 2,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "accessor-pairs": 2,
        "curly": 2,
        "eqeqeq": 1,
        "no-alert": 0
    }
}